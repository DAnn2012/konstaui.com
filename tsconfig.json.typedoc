{
  "compilerOptions": {
    "target": "ESNEXT",
    "module": "ESNEXT",
    "declaration": true,
    "declarationMap": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "noEmit": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "typedocOptions": {
    "name": "KonstaUI",
    "entryPoints": [],
    "excludeExternals": false,
    "includeVersion": true,
    "out": "./public/types"
  },
  "include": [
    "node_modules/@types/react",
    "node_modules/konsta/react/*.d.ts",
    "node_modules/konsta/react/**/*.d.ts"
  ]
}
